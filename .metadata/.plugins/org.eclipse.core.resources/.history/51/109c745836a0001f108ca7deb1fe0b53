package StaticVariable;

public class Student {
	private static int count;
	private int studId;
	private String studName;
	private int studAge;
	
	
	public Student() {
		count++;
	}
	
	public Student(int id,String name,int age) {
		count++;
		studId = studId;
		studName = studName;
		studAge = studAge;
	}
	
	public static int getcount() {
		return count;
	}
	
	
}
