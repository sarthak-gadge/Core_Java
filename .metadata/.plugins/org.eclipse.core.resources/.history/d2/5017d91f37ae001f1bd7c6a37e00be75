package Interface;

public class Circle extends Shape implements Printable {
	
	private double area;
	private double PI=3.14;
	private double radius;
	
	
	public Circle( double radius) {
		
		this.radius = radius;
	}
	
	
	@Override
	public void area() {
	    area = 3.14*radius*radius;
	}
	
	
	@Override
	public void print() {
		System.out.println("area of circle is: "+area);
		
	}
	
	
	

}
