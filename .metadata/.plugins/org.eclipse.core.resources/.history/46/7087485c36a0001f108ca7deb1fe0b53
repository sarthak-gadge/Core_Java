package StaticVariable;

public class Student2 {
	
//	static block
	
	
	private static int autoId;
	private int studId;
	private String studName;
	private int studAge;
	
	static{
		autoId=100;
	}
	
	
	public Student2() {
		autoId++;
		studId = autoId;
		studName = "sarthak";
		studAge = 22;
				
	}
	
	public Student2(String name,int age) {
		autoId++;
		studId = autoId;
		studName = studName;
		studAge = studAge;
	}
	
	
}
